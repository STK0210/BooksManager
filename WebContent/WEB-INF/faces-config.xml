<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd" version="2.3">

	<application>

		<locale-config>
			<default-locale>en</default-locale>
			<supported-locale>zh</supported-locale>
		</locale-config>

		<resource-bundle>
			<base-name>messages</base-name>
			<var>msgs</var>
		</resource-bundle>

	</application>
	<navigation-rule>
		<display-name>xhtml/index.xhtml</display-name>
		<from-view-id>/xhtml/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/xhtml/welcome.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
